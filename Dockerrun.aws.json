{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "krigar1184/complex-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "krigar1184/complex-nginx",
      "essential": true,
      "portMappings": [{
        "hostPort": 80,
        "containerPort": 80
      }],
      "links": ["client", "server"]
    },
    {
      "name": "api",
      "image": "krigar1184/complex-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "krigar1184/complex-worker",
      "hostname": "worker",
      "essential": false
    }
  ]
}
